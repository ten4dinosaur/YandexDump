  <style>
    :host {
      --movement-to-pointer-left: 0;
      --movement-to-snappoint-duration: 250ms;
      --movement-to-snappoint-transform-start: translateX(0px);
      --movement-to-snappoint-transform-end: translateX(0px);
      --state-change-initial-transform: -10px;
      --state-change-final-transform: 0px;
      --button-background-color: #2E2F34
    }

    :host {
      touch-action: none;
      z-index: 1000;
      display: block;
      position: absolute;
      left: 50%;
      top: 100%;
      transform: translateX(-50%);
      pointer-events: none;
      width: max-content;
      height: max-content
    }

    :host([ya-overvideo]) {
      top: 50%;
      transform: translate(-50%, -50%)
    }

    :host([ya-top-mode]) {
      top: 28px
    }

    :host #ya-video-ui {
      position: absolute;
      height: max-content;
      cursor: grab;
      pointer-events: auto;
      display: flex;
      justify-content: center;
      align-items: center;
      flex-flow: row nowrap;
      transform: translate(-10px);
      padding: 12px 10px;
      will-change: transform, left
    }

    #ya-video-ui[ya-hidden] {
      pointer-events: none
    }

    #ya-video-ui-clone,
    .ya-video-ui-inner {
      display: block;
      height: max-content;
      color: #fff;
      opacity: 1
    }

    #ya-video-ui[ya-hidden] .ya-video-ui-inner {
      opacity: 0;
      transition: opacity .7s ease
    }

    #ya-video-ui-clone {
      position: relative;
      visibility: hidden;
      padding: 12px 10px
    }

    #ya-video-ui[ya-shrunk] .can-shrunk {
      max-width: 0;
      padding: 0
    }

    .ya-video-ui-buttons {
      display: flex;
      justify-content: center;
      align-items: stretch;
      flex-flow: row nowrap;
      height: max-content;
      min-height: 32px;
      box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.25);
      border-radius: 12px;
      overflow: hidden
    }

    .ya-video-ui-buttons>* {
      display: flex;
      justify-content: center;
      align-items: center;
      fill: #fff;
      padding: 4px 0;
      background-color: var(--button-background-color)
    }

    .ya-video-ui-buttons>:not(.last-visible) {
      border-right: 1px solid rgba(255, 255, 255, .2)
    }

    .ya-video-ui-buttons>:nth-child(2) {
      z-index: 2
    }

    .ya-video-ui-buttons>:nth-child(3) {
      z-index: 3
    }

    .ya-video-ui-buttons>:last-child {
      border-top-right-radius: 12px;
      border-bottom-right-radius: 12px
    }

    .animate-movement-to-pointer {
      animation: 150ms linear movement-to-pointer;
    }

    @keyframes movement-to-pointer {
      from {
        left: 0;
      }

      to {
        left: var(--movement-to-pointer-left);
      }
    }

    .animate-movement-to-snappoint {
      animation: var(--movement-to-snappoint-duration, 250ms) ease-out forwards movement-to-snappoint;
    }

    @keyframes movement-to-snappoint {
      from {
        transform: var(--movement-to-snappoint-transform-start);
      }

      to {
        transform: var(--movement-to-snappoint-transform-end);
      }
    }

    .animate-state-change {
      animation: 500ms linear state-change;
    }

    @keyframes state-change {
      from {
        transform: translate(var(--state-change-initial-transform));
      }

      to {
        transform: translate(var(--state-change-final-transform));
      }
    }

    :host {
      --minimizing-animation-direction: normal;
      --minimizing-max-width: 0;
      --minimizing-padding-right: 12px;
    }

    .ya-video-button-icon-container {
      box-sizing: content-box;
      display: flex;
      justify-content: center;
      transition: fill .5s, color .5s
    }

    .ya-video-button-label,
    .ya-video-button-label-layer {
      font-family: YS Text, "Segoe UI", BlinkMacSystemFont, Arial, sans-serif;
      font-style: normal;
      font-weight: 400;
      font-size: 15px;
      line-height: 16px;
      min-height: 16px;
      letter-spacing: 0;
      text-align: left;
      hyphens: manual;
      overflow: hidden;
      user-select: none;
      white-space: nowrap
    }

    .ya-video-button-label-layer {
      position: absolute;
      top: 0;
      left: 0
    }

    .ya-video-button-label-container {
      max-width: 100%;
      padding-right: 12px;
      position: relative
    }

    #ya-video-ui .ya-video-button-label-container {
      transition: width .5s
    }

    .animate-state-change-initial-text {
      opacity: 0;
      animation: 250ms linear state-change-initial-text;
    }

    @keyframes state-change-initial-text {
      from {
        opacity: 1;
      }

      to {
        opacity: 0;
      }
    }

    .animate-state-change-final-text {
      opacity: 0;
      animation: 250ms linear state-change-final-text;
      animation-delay: 250ms;
    }

    @keyframes state-change-final-text {
      from {
        opacity: 0;
      }

      to {
        opacity: 1;
      }
    }

    .animate-minimizing {
      animation: 250ms ease-in forwards minimizing;
      animation-direction: var(--minimizing-animation-direction);
    }

    @keyframes minimizing {
      from {
        opacity: 1;
        max-width: var(--minimizing-max-width);
        padding-right: var(--minimizing-padding-right);
      }

      to {
        opacity: 0;
        max-width: 0;
        padding-right: 0;
      }
    }

    :host {
      --minimizing-logo-container-padding-left: 6px;
      --minimizing-logo-container-max-width: 24px;
      --translation-label-width: unset;
    }

    .ya-video-translation-button-alice-icon-container {
      padding-left: 6px;
      overflow: hidden
    }

    .ya-video-translation-button-translator-icon-container {
      min-width: 32px
    }

    .ya-video-translation-button-alice-icon {
      min-height: 24px;
      min-width: 24px;
      max-width: 24px;
      max-height: 24px
    }

    #ya-video-ui .ya-video-translation-button-label-container {
      width: var(--translation-label-width)
    }

    :host([active-label-name=subtitles]) .ya-video-translation-button-label-container {
      padding-right: 0;
      padding-left: 0
    }

    :host([translation-state=hidden]) .ya-video-translation-button {
      display: none
    }

    :host([translation-state=activated]) .ya-video-translation-button-flickering {
      color: #a36eff;
      fill: #a36eff
    }

    :host([translation-state="forced_processing"]) .ya-video-translation-button-flickering,
    :host([translation-state="in_queue"]) .ya-video-translation-button-flickering,
    :host([translation-state="processing"]) .ya-video-translation-button-flickering {
      animation: 2s ease-in-out infinite alternate processing;
    }

    @keyframes processing {
      from {
        fill: #d9c3ff;
        color: #d9c3ff
      }

      to {
        fill: #a36eff;
        color: #a36eff
      }
    }

    .animate-minimizing-logo-container {
      animation: 250ms ease-in forwards minimizing-logo-container;
      animation-direction: var(--minimizing-animation-direction);
    }

    @keyframes minimizing-logo-container {
      from {
        opacity: 1;
        padding-left: var(--minimizing-logo-container-padding-left);
        min-width: var(--minimizing-logo-container-max-width)
      }

      to {
        opacity: 0;
        padding-left: 0;
        min-width: 0
      }
    }

    :host {
      --subtitles-label-width: unset;
    }

    .ya-video-subtitles-button-icon-container {
      padding: 0 6px
    }

    #ya-video-ui .ya-video-subtitles-button-label-container {
      width: var(--subtitles-label-width)
    }

    :host([active-label-name=translation]) .ya-video-subtitles-button-label-container {
      padding-right: 0;
      padding-left: 0
    }

    :host([subtitles-state=hidden]) .ya-video-subtitles-button {
      display: none
    }

    :host([subtitles-state=activated]) .ya-video-subtitles-button-flickering {
      color: #a36eff;
      fill: #a36eff
    }

    .ya-video-subtitles-button-label-container {
      padding-right: 6px;
      position: relative
    }

    .ya-video-settings-button-icon-container {
      padding: 0 13px
    }

    :host([settings-state=hidden]) .ya-video-settings-button {
      display: none
    }

    :host([settings-state=activated]) .ya-video-settings-button {
      color: #a36eff;
      fill: #a36eff
